<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ants.dao.AnnouncementDao">
    <!--获取首页大分类的数据信息-->
    <select id="latestAnnouncement" resultType="com.ants.entity.Announcement">
        select * from ants_management_announcement ORDER BY annTime DESC limit 0,1
    </select>



</mapper>