<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ants.dao.LeaseDao">
    <!--根据学生账户获取此学生发布的租赁的商品-->
    <select id="myLeaseGoods" parameterType="Integer" resultType="com.ants.entity.Lease">
      SELECT goodsId,goodsName,goodsPicture,
             goodsPrice,uploadTime,goodsState,repertory
                FROM ants_goods_lease
                    WHERE goodsBelong=#{goodsBelong}


    </select>

</mapper>