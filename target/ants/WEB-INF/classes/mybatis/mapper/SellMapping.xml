<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ants.dao.SellDao">

    <!--根据学生学号获取此学生账号下的所交易完成的商品，包括闲置，租赁和赠送-->
    <select id="mySellGoods" resultType="com.ants.entity.Sell" parameterType="Integer">
        SELECT goodsId,goodsName,goodsPicture,
               goodsPrice,goodsState,sellTime,numbers
                  FROM ants_other_sell
                      WHERE goodsBelong = #{goodsBelong}
    </select>


</mapper>